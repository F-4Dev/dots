{
    "workbench.list.smoothScrolling": true,
    "editor.smoothScrolling": true,
    "terminal.integrated.smoothScrolling": true,
    "workbench.iconTheme": "catppuccin-mocha",
    "editor.minimap.autohide": "mouseover",
    "editor.scrollPredominantAxis": false,
    "apc.buttons": [],
    "apc.sidebar.titlebar": {},
    "workbench.editor.wrapTabs": true,
    "svelte.enable-ts-plugin": true,
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "explorer.confirmDragAndDrop": false,
    "redhat.telemetry.enabled": false,
    "security.workspace.trust.enabled": false,
    "zenMode.fullScreen": false,
    "zenMode.silentNotifications": false,
    "[jsonc]": {
        "editor.defaultFormatter": "denoland.vscode-deno"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.fontFamily": "'JetBrainsMono NFM'",
    "database-client.autoSync": true,
    "dineug.erd-editor.theme.appearance": "dark",
    "dineug.erd-editor.theme.grayColor": "mauve",
    "dineug.erd-editor.theme.accentColor": "violet",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "workbench.settings.applyToAllProfiles": [],
    "workbench.editor.enablePreview": false,
    "python.defaultInterpreterPath": "/bin/python",
    "workbench.activityBar.location": "top",
    "zenMode.showTabs": "none",
    "chat.commandCenter.enabled": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "php.problems.exclude": {
        "/": [
            "php_to_string_interpolation"
        ]
    },
    "explorer.confirmDelete": false,
    "[svelte]": {
        "editor.defaultFormatter": "svelte.svelte-vscode"
    },
    "[scss]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "glassit.alpha": 1,
    "editor.inlayHints.fontFamily": "JetBrainsMonoNerdFont-Light",
    "workbench.colorTheme": "Wal",
    "[shellscript]": {
        "editor.defaultFormatter": "mkhl.shfmt"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "denoland.vscode-deno"
    },
    "fiveServer.php.executable": "/usr/bin/php",
    "[yaml]": {
        "editor.defaultFormatter": "denoland.vscode-deno"
    },
    "shfmt.executablePath": "/usr/bin/shfmt",
    "editor.fontLigatures": true,
    "terminal.integrated.fontLigatures.enabled": true,
    "editor.formatOnPaste": true,
    "editor.renderWhitespace": "all",
    "editor.minimap.renderCharacters": false,
    "editor.minimap.side": "left",
    "workbench.layoutControl.enabled": false,
    "window.commandCenter": false,
    "window.menuBarVisibility": "toggle",
    "editor.minimap.enabled": false,
    "window.customTitleBarVisibility": "never",
    "zenMode.centerLayout": false
}
